export class Ponto{
    private x:number = 0
    private y:number = 0

    constructor(x: number, y:number){
        this.x = x
        this.y = y
    }

    get getX(){
        return this.x
    }

    get getY(){
        return this.y
    }

    set setX(x: number){
        this.x = x
    }

    set setY(y: number){
        this.y = y
    }
}
